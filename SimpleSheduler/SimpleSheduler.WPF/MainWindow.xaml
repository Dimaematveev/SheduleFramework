        <Button x:Name="FillingGroups" Content="filling Groups" HorizontalAlignment="Left" Margin="95,236,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>
        <Button x:Name="FillingClassrooms" Content="filling Classrooms" HorizontalAlignment="Left" Margin="255,236,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>
        <Button x:Name="ButtonOpenPair" Content="Pair" HorizontalAlignment="Left" Margin="415,158,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>
        <Button x:Name="ButtonOpenStudyDay" Content="StudyDay" HorizontalAlignment="Left" Margin="495,158,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>
        <Button x:Name="CreateScheduler" Content="Create Scheduler" HorizontalAlignment="Left" Height="27" Margin="175,271,0,0" VerticalAlignment="Top" Width="169"/>

    </Grid>
</Window>
