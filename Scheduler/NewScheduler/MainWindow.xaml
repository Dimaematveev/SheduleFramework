<Window x:Class="NewScheduler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewScheduler"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1060">
    <Grid>
        <Button x:Name="ButtonOn" Content="Button" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="100"/>

        <TextBox x:Name="TextBoxMaxClassroom" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="100" Height="50"/>
        <TextBox x:Name="TextBoxUnionAll" HorizontalAlignment="Left" Margin="120,10,0,10" Width="300"/>
        <TextBox x:Name="TextBoxUnionFilterSeminar" HorizontalAlignment="Left" Margin="430,10,0,10" Width="300"/>
        <TextBox x:Name="TextBoxUnionFilterPotok" HorizontalAlignment="Left" Margin="740,10,0,10" Width="300"/>

    </Grid>
</Window>
